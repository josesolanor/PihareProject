@model IEnumerable<WebPihare.Entities.Departmenttype>

@{
    ViewData["Title"] = "Index";
}

<h2>Tipo de Departamentos</h2>

<p>
    <a asp-action="Create">Crear nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DepartmentTypeValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DepartmentTypeDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentTypeValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentTypeDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DepartmentTypeId">Editar</a> |
                
                <a asp-action="Delete" asp-route-id="@item.DepartmentTypeId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
